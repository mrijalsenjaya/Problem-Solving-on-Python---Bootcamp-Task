        "def prime_check(x):\n",
        "  if x == 1 :\n",
        "    print(x, \"is NOT a Prime Number\")\n",
        "  elif x == 2 :\n",
        "    print(x, \"is a Prime Number\")\n",
        "  elif x % 2 != 0 :\n",
        "    print(x, \"is a Prime Number\")\n",
        "  else:\n",
        "    print(x, \"is NOT a Prime Number\")"
